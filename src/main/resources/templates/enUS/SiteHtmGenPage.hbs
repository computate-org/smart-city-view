					type="text"
					class="suggestSiteHtm w3-input w3-border w3-bar-item "
					name="suggestSiteHtm"
					id="suggestSiteHtm{{id}}"
					autocomplete="off"
					oninput="suggestSiteHtmObjectSuggest( [ { 'name': 'q', 'value': 'objectSuggest:' + $(this).val() }, { 'name': 'rows', 'value': '10' }, { 'name': 'fl', 'value': 'classCanonicalName,,pageUrlPk,objectTitle' } ], $('#suggestListSiteHtm{{id}}'), {{id}}; "
					onkeyup="if (event.keyCode === 13) { event.preventDefault(); window.location.href = '/htm?q={{query1}}:' + encodeURIComponent(this.value) + '{{fqs}}{{sorts}}&amp;rows={{start2}}&amp;rows={{rows1}}"
				{{#if listSiteHtm}}
					value="{{query2}}"
				{{/if}}
				/>
