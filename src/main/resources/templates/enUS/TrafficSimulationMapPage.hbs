{{#partial "htmBodyMiddle"}}
		<div id="map"></div>
		<canvas style="display: none"></canvas>

		<div id="side">
			<div id="tabs">
				<div class="tab" title="Options">
					<img src="{{ STATIC_BASE_URL }}/png/webWizard/generate.png" />
				</div>
				<div class="tab" title="Vehicles">
					<img src="{{ STATIC_BASE_URL }}/png/webWizard/passenger.png" />
				</div>
				<div class="tab" title="Copyright">Â©</div>
			</div>

			<div class="controls">
				<div class="container">
					<h4>Position</h4>

					<div class="options open">
					  <p>
						<input id="address" value="Berlin" size="20"/>
						<input id="buttonSearch" type="button" value="Search"/> </br>
					  </p>
					  <p>
						<input id="lat_lon" value="{{ DEFAULT_MAP_LOCATION.lat }} {{ DEFAULT_MAP_LOCATION.lon }}" size="{{ DEFAULT_MAP_ZOOM }}"/>
						<input id="buttonLatLon" type="button" value="Go to"/> </br>
					  </p>
					  <p>
						<input id="buttonCurrent" type="button" value="Use current location"/></br>
					  </p>
					</div>
				</div>

				<div class="container">
					<div class="options open">
						<label>Select Area <input type="checkbox" id="canvas-toggle" /></label>
					</div>
				</div>

				<div class="container" style="margin-top: 20px;">
					<h4>Options</h4>

					<div class="options open">
						<label>Add Polygons <input id="polygons" type="checkbox" checked /></label>
						<label>Duration <input id="duration" type="number" value="3600" min="0" step="1" /></label>
						<label>Import Public Transport <input id="publicTransport" type="checkbox" /></label>
						<label>Car-only Network <input id="carOnlyNetwork" type="checkbox" /></label>
						<label>Satellite background <input id="decal" type="checkbox" /></label>
						<label>Left Hand Traffic <input id="leftHand" type="checkbox" /></label>
					</div>
				</div>
			</div>

			<div class="controls" id="vehicle-controls"></div>

			<div class="controls">
				<div class="container">
					<h4>Icon Copyrights</h4>

					<div class="images">
						<img src="{{ STATIC_BASE_URL }}/png/webWizard/generate.png" />
						<img src="{{ STATIC_BASE_URL }}/png/webWizard/passenger.png" />
						<img src="{{ STATIC_BASE_URL }}/png/webWizard/truck.png" />
						<img src="{{ STATIC_BASE_URL }}/png/webWizard/bicycle.png" />
					</div>

					<p>
						Icons by <a href="http://google.com/design/">Google Design</a>,
						licensed under <a href="http://creativecommons.org/licenses/by-sa/3.0/">CC-BY-SA</a>
					</p>

					<div class="images">
						<img src="{{ STATIC_BASE_URL }}/png/webWizard/bus.png" />
						<img src="{{ STATIC_BASE_URL }}/png/webWizard/pedestrian.png" />
						<img src="{{ STATIC_BASE_URL }}/png/webWizard/ship.png" />
						<img src="{{ STATIC_BASE_URL }}/png/webWizard/rail.png" />
					</div>

					<p>
						Icons by <a href="http://aiga.org/">AIGA</a>,
						Open Domain
					</p>

					<div class="images">
						<img src="{{ STATIC_BASE_URL }}/png/webWizard/tram.png" />
						<img src="{{ STATIC_BASE_URL }}/png/webWizard/rail_urban.png" />
					</div>

					<p>
						Icons by <a href="http://www.danilodemarco.com/">Pittogrammi</a>
					</p>

					<div class="images">
						<img src="{{ STATIC_BASE_URL }}/png/webWizard/motorcycle.png" />
					</div>

					<p>
						Icon by <a href="http://icons8.com/">Icons8</a>
					</p>
				</div>
			</div>

			<div id="fixed">
				<button id="export-button">Generate Scenario</button>
				<div id="status">
					<span></span>
					<div></div>
				</div>
			</div>
		</div>
