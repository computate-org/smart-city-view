{{#*inline "htmScriptSimulationReportPage"}}		<!-- inline "htmScriptSimulationReportPage" -->
		<script>
			$('#site-calendar-box').accordion({ collapsible: true, active: false });
{{#if DEFAULT_MAP_LOCATION }}
			window.DEFAULT_MAP_LOCATION = { lat: {{ DEFAULT_MAP_LOCATION.lat }}, lon: {{ DEFAULT_MAP_LOCATION.lon }} };
{{/if}}
{{#if DEFAULT_MAP_ZOOM }}
			window.DEFAULT_MAP_ZOOM = {{ DEFAULT_MAP_ZOOM }};
{{/if}}
			window.DEFAULT_ZONE_ID = '{{ defaultZoneId }}';
			$(document).ready(function() {
				document.onkeydown = function(evt) {
					evt = evt || window.event;
					var isEscape = false;
					if ('key' in evt) {
						isEscape = (evt.key === 'Escape' || evt.key === 'Esc');
					} else {
						isEscape = (evt.keyCode === 27);
					}
					if (isEscape) {
						$('.w3-modal:visible').hide();
					}
				};
				window.eventBus = new EventBus('/eventbus');
				var pk = {{#if pk}}{{pk}}{{else}}null{{/if}};
				if(pk != null) {
		{{#ifContainsAnyRoles roles roleForWrite}}
					suggestSimulationReportSimulationKey([{'name':'fq','value':'reportKeys:' + pk}], $('#listSimulationReportSimulationKey_Page'), pk, true);
		{{else}}
		{{#ifContainsAnyRoles roles roleForRead}}
					suggestSimulationReportSimulationKey([{'name':'fq','value':'reportKeys:' + pk}], $('#listSimulationReportSimulationKey_Page'), pk, true);
		{{else}}
					suggestSimulationReportSimulationKey([{'name':'fq','value':'reportKeys:' + pk}], $('#listSimulationReportSimulationKey_Page'), pk, false);
		{{/ifContainsAnyRoles}}
		{{/ifContainsAnyRoles}}
		{{#ifContainsAnyRoles roles roleForWrite}}
					suggestSimulationReportSmartTrafficLightKey([{'name':'fq','value':'reportKeys:' + pk}], $('#listSimulationReportSmartTrafficLightKey_Page'), pk, true);
		{{else}}
		{{#ifContainsAnyRoles roles roleForRead}}
					suggestSimulationReportSmartTrafficLightKey([{'name':'fq','value':'reportKeys:' + pk}], $('#listSimulationReportSmartTrafficLightKey_Page'), pk, true);
		{{else}}
					suggestSimulationReportSmartTrafficLightKey([{'name':'fq','value':'reportKeys:' + pk}], $('#listSimulationReportSmartTrafficLightKey_Page'), pk, false);
		{{/ifContainsAnyRoles}}
		{{/ifContainsAnyRoles}}
				}
				window.varsFq = JSON.parse('{{{toJsonObjectStringInApostrophes varsFq}}}');
				window.varsRange = JSON.parse('{{{toJsonObjectStringInApostrophes varsRange}}}');
				window.defaultRangeVar = '{{ defaultRangeVar }}';
				pageGraphSimulationReport();

				var calendarEl = document.getElementById('site-calendar');
				var calendar = new FullCalendar.Calendar(calendarEl, {
					initialView: 'dayGridMonth'
				});
				//calendar.render();
{{#block "websocket"}}{{/block}}
			});
		</script>
