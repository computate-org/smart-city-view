{{#*inline "htmReportStatus"}}
<!-- #*inline "htmReportStatus" -->
										<tr>
												<td>
													<label for="{{classApiMethodMethod}}_reportStatus">report status</label>
												</td>
												<td>
														{{> "inputReportStatus" roleForWrite=roleForWrite roleForRead=roleForRead}}
												</td>
												<td>The current status of the simulation report. </td>
										</tr>
{{/inline}}

{{#*inline "inputReportStatus"}}
		{{#eq roleForWriteBool "true"}}
														<input
															type="text"
															placeholder="report status"
															title="The current status of the simulation report. "															id="{{classApiMethodMethod}}_reportStatus"
	{{#eq "Page" classApiMethodMethod}}
																class="setReportStatus classSimulationReport inputSimulationReport{{pk}}ReportStatus w3-input w3-border "
																name="setReportStatus"
	{{else}}
		{{#eq "PATCH" classApiMethodMethod}}
																class="setReportStatus classSimulationReport inputSimulationReport{{pk}}ReportStatus w3-input w3-border "
																name="setReportStatus"
		{{else}}
																class="valueReportStatus w3-input w3-border classSimulationReport inputSimulationReport{{pk}}ReportStatus w3-input w3-border "
																name="reportStatus"
		{{/eq}}
	{{/eq}}
	{{#eq "Page" classApiMethodMethod}}
																onclick="removeGlow($(this)); "
																onchange="patch{{classSimpleName}}Val([{ name: 'softCommit', value: 'true' }, { name: 'fq', value: 'pk:{{pk}}' }], 'setReportStatus', $(this).val(), function() { addGlow($('#{{classApiMethodMethod}}_reportStatus')); }, function() { addError($('#{{classApiMethodMethod}}_reportStatus')); }); "
	{{/eq}}
	{{#eq "Page" classApiMethodMethod}}
															value="{{simulationReport_.reportStatus}}"
	{{/eq}}
														/>

													{{else}}
														{{#ifContainsKeys userKeys}}
															<span class="varSimulationReport{{pk}}ReportStatus varSimulationReportReportStatus ">{{simulationReport_.reportStatus}}</span>
			{{/ifContainsKeys}}
		{{else}}
														<span class="varSimulationReport{{pk}}ReportStatus varSimulationReportReportStatus ">{{simulationReport_.reportStatus}}</span>
		{{/eq}}
{{/inline}}

